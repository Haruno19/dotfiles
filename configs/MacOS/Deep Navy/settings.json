{
  "window.restoreWindows": "all",
  "files.hotExit": "onExit",
  "workbench.startupEditor": "none",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
      "*.pl": "prolog"
  },
  "editor.suggestSelection": "first",
  "files.exclude": {
      "**/.classpath": true,
      "**/.project": true,
      "**/.settings": true,
      "**/.factorypath": true,
      "**/.vscode": true
  },
  "editor.fontSize": 13,
  "editor.fontFamily": "Menlo, Cascadia Mono Light, Space Mono, Victor Mono, Monaco, 'Courier New', monospace",
  "workbench.colorTheme": "Material Theme Ocean High Contrast",
  "workbench.productIconTheme": "icons-carbon",
  "workbench.colorCustomizations": {
    "list.activeSelectionBackground": "#191919",
    "list.hoverBackground": "#191919",
    "activityBar.border": "#0e0f16",
    "tab.inactiveBackground": "#12141c",
    "tab.activeBorder": "#0e0f1600",
    "editorGroupHeader.tabsBackground": "#12141c",
    "editorWidget.background": "#0e0f16",
    "panel.background": "#0e0f16",
    "titleBar.activeBackground": "#0e0f16",
    "breadcrumbPicker.background": "#0e0f16",
    "settings.dropdownBackground": "#0e0f16",
    "settings.numberInputBackground": "#0e0f16",
    "settings.textInputBackground": "#0e0f16",
    "settings.checkboxBackground": "#0e0f16",
    "tab.activeBackground": "#0e0f16",
    "sideBarSectionHeader.background": "#0e0f16",
    "activityBar.background": "#0e0f16",
    "statusBar.background": "#0e0f16",
    "scrollbarSlider.activeBackground": "#0e0f16",
    "editor.background": "#12141c",
    "sideBar.background": "#0e0f16",
    "breadcrumb.background": "#12141c",
    "editorHoverWidget.background": "#0e0f16",
    "toolbar.hoverBackground": "#0e0f16",
    "notebook.cellStatusBarItemHoverBackground": "#0e0f16",
    "button.hoverBackground": "#0e0f16",
    "inputOption.hoverBackground": "#0e0f16",
    "sash.hoverBorder": "#0e0f16",
    "settings.rowHoverBackground": "#0e0f16",
    "sideBar.dropBackground": "#0e0f16",
    "editorHoverWidget.statusBarBackground": "#0e0f16",
    "menu.selectionBackground": "#0e0f16",
    "selection.background": "#0e0f16",
    "list.inactiveSelectionBackground": "#0e0f16",
    "menubar.selectionBackground": "#0e0f16",
    "peekViewResult.selectionBackground": "#0e0f16"
  },
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "customizeUI.activityBar": "bottom",
  "customizeUI.titleBar": "inline",
  "customizeUI.font.regular": "Menlo",
  "customizeUI.fontSizeMap": {
      "13px": "12px",
      "monospace": "12px"
  },
  "customizeUI.stylesheet": {
    ".tabs-container ": "padding-top:7px; padding-left:2px; padding-bottom:5px",
    ".tabs-container > .tab": "border-radius: 7px 7px 7px 7px; height: 20px; margin-left: 5px; padding-right:4px",
    ".monaco-editor .suggest-widget": "border-radius: 10px 10px 10px 10px; overflow:hidden;"
  },
  "window.titleBarStyle": "native",
  "editor.minimap.enabled": false,
  "security.workspace.trust.enabled": false
}